        <pattern>/admin/resourcesList</pattern> 
        <pattern>/admin/resourceEdit</pattern> 
        <pattern>/admin/addNewImage</pattern> 
        <pattern>/admin/addNewVideo</pattern> 
        <pattern>/admin/linksList</pattern> 
