<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<decorators defaultdir="/WEB-INF/sitemesh-decorators">
	<excludes>
	    <pattern>/j_spring_security_logout</pattern>
	    <pattern>/pages/*</pattern>
	    <pattern>/j_spring_security_check</pattern>
